<section class="container-fluid results">
  <div class="container">
    <div class="row">
      <div class="col-12 col-sm-10">
        <h1 class="title">{{spot.name}}</h1>
        <h2>Description</h2>
        <p>{{spot.content}}</p>
        <span class="avatar"
          style="display: block; width:6rem; height:6rem; background: url({{spot.creatorId.avatar}})"></span>
        <p>posted by @{{spot.creatorId.username}}</p>

        {{#each spot.creatorId.pets as |pet|}}
        <span class="avatar" style="display: block; width:6rem; height:6rem; background: url({{pet.picPath}})"></span>
        {{/each}}

        <hr>

        <h2>Comments</h2>
        {{#each spot.comments as |comment|}}

        <p>{{comment.content}}</p>
        <span class="avatar"
          style="display: block; width:6rem; height:6rem; background: url({{authorId.avatar}})"></span>
        <p>posted by @{{authorId.username}}</p>
        {{/each}}

        {{#each spot.category as |category|}}
        <p>{{category}}</p>
        {{/each}}

        <h2>Website</h2>
        <p>{{spot.url}}</p>

        <h2>Rate</h2>
        <p>{{spot.rate}} / 5</p>

        <h2>Addres</h2>
        <p>{{spot.address}}, {{spot.city}}, {{spot.zipCode}}</p>


        <iframe width="620" height="350" scrolling="no" frameborder="0" marginheight="0" marginwidth="0"
          src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=es&amp;geocode=&amp;q={{spot.address}}, {{spot.city}}&amp;iwloc=near&amp;z=15&amp;output=embed"></iframe>

        <a href="https://www.google.com/maps/search/{{spot.address}},+{{spot.zipCode}}+{{spot.city}}/" target="_new">ver
          mapa</a>

        <h2>Working hours</h2>
        <p><strong>Days</strong> {{spot.days}}</p>
        <p><strong>Open</strong> {{spot.open}}</p>
        <p><strong>Close</strong> {{spot.close}}</p>

        <h2>Contact</h2>
        <p><strong>Phone</strong> {{spot.phone}}</p>
        <p><strong>Instagram</strong><a href="https://www.instagram.com/{{spot.instagram}}" target="_new"><svg
              aria-hidden="true" focusable="false" data-prefix="fab" data-icon="instagram" role="img"
              xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"
              class="svg-inline--fa fa-instagram fa-w-14 fa-3x">
              <path fill="currentColor"
                d="M224.1 141c-63.6 0-114.9 51.3-114.9 114.9s51.3 114.9 114.9 114.9S339 319.5 339 255.9 287.7 141 224.1 141zm0 189.6c-41.1 0-74.7-33.5-74.7-74.7s33.5-74.7 74.7-74.7 74.7 33.5 74.7 74.7-33.6 74.7-74.7 74.7zm146.4-194.3c0 14.9-12 26.8-26.8 26.8-14.9 0-26.8-12-26.8-26.8s12-26.8 26.8-26.8 26.8 12 26.8 26.8zm76.1 27.2c-1.7-35.9-9.9-67.7-36.2-93.9-26.2-26.2-58-34.4-93.9-36.2-37-2.1-147.9-2.1-184.9 0-35.8 1.7-67.6 9.9-93.9 36.1s-34.4 58-36.2 93.9c-2.1 37-2.1 147.9 0 184.9 1.7 35.9 9.9 67.7 36.2 93.9s58 34.4 93.9 36.2c37 2.1 147.9 2.1 184.9 0 35.9-1.7 67.7-9.9 93.9-36.2 26.2-26.2 34.4-58 36.2-93.9 2.1-37 2.1-147.8 0-184.8zM398.8 388c-7.8 19.6-22.9 34.7-42.6 42.6-29.5 11.7-99.5 9-132.1 9s-102.7 2.6-132.1-9c-19.6-7.8-34.7-22.9-42.6-42.6-11.7-29.5-9-99.5-9-132.1s-2.6-102.7 9-132.1c7.8-19.6 22.9-34.7 42.6-42.6 29.5-11.7 99.5-9 132.1-9s102.7-2.6 132.1 9c19.6 7.8 34.7 22.9 42.6 42.6 11.7 29.5 9 99.5 9 132.1s2.7 102.7-9 132.1z"
