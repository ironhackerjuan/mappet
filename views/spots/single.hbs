        <hr class="mb-5">
        <div class="col-12">
          <h2 class="text-center mb-3">Working hours</h2>
          <div class="row justify-content-center">
            <p class="col-12 col-sm-3"><strong>Days</strong> {{spot.days}}</p>
            <p class="col-12 col-sm-3"><strong>Open at</strong> {{spot.open}}</p>
            <p class="col-12 col-sm-3"><strong>Close at</strong> {{spot.close}}</p>
          </div>
        </div>
        <hr>

        {{#if spot.rate}}
        <div class="rate-info">
          <h2>{{spot.name}}</h2> has received a rating of {{spot.rate}} out of 5 bones by
          {{spot.comments.length}} critics on Mappet.
        </div>

        <hr>

        {{/if}}
        {{#if spot.comments}}
        <h2 class="title">{{spot.comments.length}} reviews</h2>
        {{else}}
        <h2 class="title">No reviews yet</h2>
        {{/if}}

        {{#each spot.comments as |comment|}}
        <div class="col-12 comments">
          <div class="row">
            <div class="col-2">
              <span class="avatar" style="background: url({{authorId.avatar}}) center center / cover"></span>
              <small>member since</small>
              <span class="date">{{date authorId.createdAt}}</span>
            </div>
            <div class="col-10">
              <p class="content"><a href="/user/{{authorId._id}}">@{{authorId.username}}</a> wrote:</p>
              <p>{{comment.content}}</p>
            </div>
          </div>
        </div>
        <hr>
        {{/each}}

        {{#unless owner}}
        <h2>wanna comment?</h2>
        <div class="col-12 comments">
          <div class="row">
            {{#if user}}
            <div class="col-2">
              <span class="avatar" style="background: url({{user.avatar}}) center center / cover"></span>
              <small>member since</small>
              <span class="date">{{date user.createdAt}}</span>
            </div>
            {{/if}}
            <div class="col-10">
              <form action="/{{spot.category}}/{{spot._id}}/comment" class="make-comment" method="POST">
                <div class="form-group col-12 col-sm-5 ranger">
                  <div class="range-slider">
                    <label for="open">Rate this spot</label>
                    <input class="custom-range range-slider__range" type="range" value="5" min="1" max="5" step="1"
                      name="rate">
                    <span class="range-slider__value">5</span>
                  </div>
                </div>
                <div class="form-group">
                  <span class="textarea">
                    <textarea type="text" class="form-control" id="content" name="content" rows="5"></textarea>
                  </span>
                </div>
                <button type="submit" class="btn">
                  <svg aria-hidden="true" focusable="false" data-prefix="far" data-icon="arrow-from-bottom" role="img"
                    xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512">
                    <path fill="#fff"
                      d="M35.5 183.9l148-148.4c4.7-4.7 12.3-4.7 17 0l148 148.4c4.7 4.7 4.7 12.3 0 17l-19.6 19.6c-4.8 4.8-12.5 4.7-17.1-.2L218 123.2V372c0 6.6-5.4 12-12 12h-28c-6.6 0-12-5.4-12-12V123.2l-93.7 97.1c-4.7 4.8-12.4 4.9-17.1.2l-19.6-19.6c-4.8-4.7-4.8-12.3-.1-17zM372 428H12c-6.6 0-12 5.4-12 12v28c0 6.6 5.4 12 12 12h360c6.6 0 12-5.4 12-12v-28c0-6.6-5.4-12-12-12z"
                      class=""></path>
                  </svg>
                </button>
              </form>
            </div>
          </div>
        </div>
        {{/unless}}
      </div>
      <aside class="col-12 col-sm-4 user-info">
        <div class="d-flex justify-content-start">
          <span class="avatar" style="background: url({{spot.creatorId.avatar}}) no-repeat center center / cover">
          </span>
          <span class="spot-info">
            <p><strong>posted by</strong>
              <a href="/user/{{spot.creatorId._id}}">@{{spot.creatorId.username}}</a>
            </p>
            <p>{{date spot.createdAt}}</p>
            <span class="d-flex justify-content-center mt-3">
              {{#each spot.creatorId.pets as |pet|}}
              <span class="avatar pet" style="background: url({{pet.avatar}}) no-repeat center center /cover;"></span>
              {{/each}}
            </span>
          </span>
        </div>

        {{#if owner}}
        <a href="/{{spot.category.[0]}}/{{spot._id}}/update" class="edit-spot">Edit spot</a>
        <a href="/{{spot.category.[0]}}/{{spot._id}}/delete" class="delete-spot">delete spot</a>
        {{/if}}

        <div class="col-12 aside-iframe">
          <hr>
          {{#if spot.address}}
          <h1 class="title">see in map</h1>
          <iframe scrolling="no" frameborder="0" marginheight="0" marginwidth="0"
            src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=es&amp;geocode=&amp;q={{spot.address}}&amp;iwloc=near&amp;z=15&amp;output=embed"></iframe>
          {{/if}}
        </div>

        <div class="col-12 aside-cats">
          <h1 class="title">lastest {{spot.category}} added</h1>
          {{#if lastest}}
          <ul>
            {{#each lastest as |last|}}
            <li>
              <a class="aside-lastest" href="/">
                <div class="last-bg" style="background: url({{last.pictures}}) no-repeat center center / cover;"></div>
                <span>
                  <p>{{last.name}}</p>
                  <small><strong>added</strong> {{date last.createdAt}}</small>
                </span>
              </a>
            </li>
            <hr>
            {{/each}}
          </ul>
          {{else}}
          <p>Nobody mappet more {{spot.category}} yet...</p>
          {{/if}}
        </div>
      </aside>
    </div>
  </div>
</section>

<!-- Modal -->
<div class="modal fade" id="mapModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <span class="info"><strong>{{spot.name}}</strong><br>{{spot.address}}, {{spot.city}} - {{spot.zipCode}}</span>
    <div class="modal-content">
      <div class="modal-body">
        <iframe scrolling="no" frameborder="0" marginheight="0" marginwidth="0"
          src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=es&amp;geocode=&amp;q={{spot.address}}, {{spot.city}}&amp;iwloc=near&amp;z=15&amp;output=embed"></iframe>
      </div>
    </div>
  </div>
</div>

<script>
  document.querySelector('.carousel-item').classList.add('active')
</script>


