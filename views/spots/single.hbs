<section class="container-fluid results single-results">
  <div class="container">
    <div class="row">
      <div class="col-12 col-sm-8 spot-info">
        {{#each spot.category as |category|}}
        <span class="cat-label">
          <a href="/{{category}}" class="label-pill" tabindex="0">{{category}}</a>
        </span>
        {{/each}}
        {{#if user}}
        <button class="likes-btn {{#if spot.likes}}fill-hearth{{/if}}" id="{{spot._id}}" onclick="like(event)">
          <span class="likes-count">{{spot.likes.length}}</span> likes
        </button>
        {{else}}
        <a href="/login" class="likes-btn" id="{{spot._id}}">
          <span class="likes-count">{{spot.likes.length}}</span> likes
        </a>
        {{/if}}
        <h1 class="title">{{spot.name}}</h1>
        <div class="map-social">
          {{#if spot.address}}
          <button type="button" class="map-link" data-toggle="modal" data-target="#mapModal">
            {{spot.address}}, {{spot.city}} - {{spot.zipCode}}
          </button>
